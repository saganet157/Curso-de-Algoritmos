#include <stdio.h>
#include <stdlib.h>

int main()

{
//declarei as variaveis

    float largura, comprimento, valorMetro, areaTerreno, precoTerreno;


//Entrada de dados

    printf("Digite a largura do terreno: ");
        scanf("%f",&largura);

    printf("Digite o comprimento do terreno: ");
        scanf("%f", &comprimento);

    printf("Digite o valor do metro quadrado: ");
        scanf("%f", &valorMetro);

        areaTerreno = largura * comprimento;

        precoTerreno = areaTerreno * valorMetro;

//Saida de Dados

    printf("\nArea do terreno: %0.2f", areaTerreno);

    printf("\nPreco do terreno: %0.2f",precoTerreno);


    return 0;
}
